			</result>
			<result name="fail" type="json">
				<param name="root">dataMap</param>
			</result>
		</action>
		
		<action name="generateParam" class="com.vm.test.GenerateParam" method="generateParamFile">
			<result name="success" type="json">
				<param name="root">dataMap</param>
			</result>
			<result name="fail" type="json">
				<param name="root">dataMap</param>
			</result>
		</action>
		
		<action name="deleteFile" class="com.vm.test.GenerateParam" method="deleteFile">
