			</result>
		</action>
		
		<action name="getvdbenchparamFile" class="com.vm.test.DealAction" method="getVdbenchParamFile">
			<result name="success" type="json">
				<param name="root">dataMap</param>
			</result>
			<result name="fail" type="json">
				<param name="root">dataMap</param>
			</result>
		</action>
		
		<action name="killtest" class="com.vm.test.DealAction" method="killTest">
			<result name="success" type="json">
				<param name="root">dataMap</param>
			</result>
			<result name="fail" type="json">
				<param name="root">dataMap</param>
			</result>
		</action>
		
