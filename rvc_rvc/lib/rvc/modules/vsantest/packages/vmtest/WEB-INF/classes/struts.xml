			</result>
			<result name="fail" type="json">
				<param name="root">dataMap</param>
			</result>
		</action>
		
		<action name="istestfinish" class="com.vm.test.DealAction" method="isTestFinished">
			<result name="success" type="json">
				<param name="root">dataMap</param>
			</result>
			<result name="fail" type="json">
				<param name="root">dataMap</param>
			</result>
		</action>
		
		<action name="hasvdbenchfile" class="com.vm.test.DealAction" method="hasVdbenchFile">
			<result name="success" type="json">
				<param name="root">dataMap</param>
			</result>
			<result name="fail" type="json">
				<param name="root">dataMap</param>
			</result>
		</action>
		
