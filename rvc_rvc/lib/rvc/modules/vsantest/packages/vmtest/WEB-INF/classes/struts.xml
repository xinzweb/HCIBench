<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<constant name="struts.multipart.saveDir" value="/tmp" />
	<constant name="struts.i18n.encoding" value="utf-8" />
	<constant name="struts.locale" value="utf-8" />
	<constant name="struts.multipart.maxSize" value="10000000000"></constant>

	<package name="index" extends="json-default" namespace="/">
	    <action name="index" class="com.vm.test.DealAction" method="index">
			<result >/config/index.html</result>
		</action>
	
		<action name="generatefile" class="com.vm.test.DealAction" method="generateFile">
			<result name="success" type="json">
				<param name="root">dataMap</param>
			</result>
			<result name="fail" type="json">
				<param name="root">dataMap</param>
			</result>
		</action>
		
		<action name="runtest" class="com.vm.test.DealAction" method="runTest">
			<result name="success" type="json">
				<param name="root">dataMap</param>
			</result>
			<result name="fail" type="json">
				<param name="root">dataMap</param>
			</result>
		</action>
		
		<action name="validatefile" class="com.vm.test.DealAction" method="validateFile">
			<result name="success" type="json">
				<param name="root">dataMap</param>
			</result>
			<result name="fail" type="json">
				<param name="root">dataMap</param>
			</result>
		</action>
		
		<action name="uploadvdbench" class="com.vm.test.DealAction" method="uploadVdbench">
			<result name="success" type="json">
				<param name="root">dataMap</param>
			</result>
			<result name="fail" type="json">
				<param name="root">dataMap</param>
			</result>
		</action>
		
		<action name="uploadParamfile" class="com.vm.test.DealAction" method="uploadParamfile">
